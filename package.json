{
  "name": "auth-graphql-boilerplate",
  "version": "1.0.0",
  "description": "PostgreSQL Prisma GraphQL authentication boilerplate",
  "main": "src/index.js",
  "author": "Austin Howlett <austinhowletttech@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "ora": "^3.4.0",
    "prisma-client-lib": "^1.33.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "dotenv-cli": "^2.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.1",
    "typescript": "^3.4.5"
  },
  "resolutions": {
    "graphql": "^14.2.0"
  }
}
